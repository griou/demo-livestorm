{
  "name": "nodejs-wdio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "ci:bs": "wdio tests/config/wdio.bs.conf.js",
    "ci:e2e": "npm run ci:mobile && npm run ci:desktop",
    "ci:mobile": "VIEWPORT=mobile wdio tests/config/wdio.ci.conf.js",
    "ci:desktop": "VIEWPORT=desktop wdio tests/config/wdio.ci.conf.js",
    "e2e": "npm run report:clean && npm run e2e:mobile ; npm run e2e:desktop",
    "e2e:desktop": "VIEWPORT=desktop wdio tests/config/wdio.local.conf.js",
    "e2e:mobile": "VIEWPORT=mobile wdio tests/config/wdio.local.conf.js",
    "e2e:debug": "DEBUG=1 VIEWPORT=desktop npm run e2e:desktop",
    "e2e:up": "docker-compose -f docker-compose.e2e.yml up -d",
    "e2e:down": "docker-compose -f docker-compose.e2e.yml down",
    "report": "npm run report:make && npm run report:open ",
    "report:clean": "rm -rf allure-*",
    "report:open": "allure open",
    "report:make": "allure generate allure-report-e2e --clean allure-results",
    "test": "npm run e2e"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "@wdio/allure-reporter": "^6.1.14",
    "@wdio/browserstack-service": "^6.1.15",
    "@wdio/cli": "^6.1.17",
    "@wdio/junit-reporter": "^6.1.14",
    "@wdio/local-runner": "^6.1.17",
    "@wdio/mocha-framework": "^6.1.17",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "@wdio/shared-store-service": "^6.1.4",
    "@wdio/spec-reporter": "^6.1.14",
    "@wdio/sync": "^6.1.14",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
